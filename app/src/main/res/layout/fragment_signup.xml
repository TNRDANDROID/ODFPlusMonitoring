                    <android.support.v7.widget.AppCompatAutoCompleteTextView
                        android:id="@+id/motivator_account_tv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter Account No"
                        android:layout_alignParentStart="true"
                        android:layout_centerInParent="true"
                        android:layout_marginLeft="15dp"
                        android:background="@android:color/transparent"
                        android:inputType="number"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/_15sdp"
                        android:visibility="visible"
                        customfontdemo:fontName="@string/font_circular_book" />


                </RelativeLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/bank_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.nic.ODFPlusMonitoring.Support.MyCustomTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/margin_left_20dp"
                    android:layout_marginTop="@dimen/padding"
                    android:layout_marginRight="@dimen/margin_left_20dp"
                    android:text="Enter Bank Name"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/_15sdp"
                    android:visibility="visible"
