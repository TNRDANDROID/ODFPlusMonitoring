<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:customfontdemo="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/blue"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/header_layout"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/colorPrimary">

        <com.nic.ODFPlusMonitoring.Support.MyCustomTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginTop="@dimen/margin"
            android:text="Registration"
            android:textColor="@android:color/white"
            android:textSize="18sp"
            customfontdemo:fontName="@string/font_circular_medium" />


    </RelativeLayout>

    <View
        android:id="@+id/horizontal_line"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@+id/header_layout"
        android:background="#FFF"
        android:visibility="gone" />

    <ScrollView
        android:id="@+id/scroll_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/horizontal_line"

        android:layout_alignParentStart="true"

        android:layout_marginStart="0dp"
        android:layout_marginTop="-1dp"
        android:fadeScrollbars="false">

        <LinearLayout
            android:id="@+id/child_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="250dp"
                android:layout_height="200dp"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/margin"
                android:gravity="center">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/profile_image"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/padding"
                    android:background="@drawable/profile_bg_circle"
                    android:scaleType="centerCrop"/>

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/profile_image_preview"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_margin="@dimen/padding"
                    android:layout_gravity="center"
                    android:background="@drawable/profile_bg_circle"
                    android:scaleType="centerCrop"
                    android:src="@drawable/ic_account_circle_black_24dp" />


                <RelativeLayout
                    android:id="@+id/edit_image"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="bottom|right"
                    android:layout_margin="@dimen/_37sdp"
                    android:background="@drawable/edit_profile_bg">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_centerInParent="true"
                        android:src="@android:drawable/ic_menu_edit" />

                </RelativeLayout>

            </FrameLayout>


            <LinearLayout
                android:id="@+id/name_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.nic.ODFPlusMonitoring.Support.MyCustomTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/margin_left_20dp"
                    android:layout_marginTop="@dimen/padding"
                    android:layout_marginRight="@dimen/margin_left_20dp"
                    android:text="Enter Name"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/_15sdp"
                    android:visibility="visible"
                    customfontdemo:fontName="@string/font_circular_medium" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/header"
                    android:layout_marginLeft="@dimen/margin_left_20dp"
                    android:layout_marginTop="@dimen/padding"
                    android:layout_marginRight="@dimen/margin_left_20dp"
                    android:background="@drawable/rectangle_box"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <com.nic.ODFPlusMonitoring.Support.MyEditTextView
                        android:id="@+id/motivator_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_centerInParent="true"
                        android:layout_marginLeft="15dp"
                        android:background="@android:color/transparent"
                        android:inputType="textPersonName"
                        android:singleLine="true"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/_15sdp"
                        android:hint="Enter Name"

                        customfontdemo:fontName="@string/font_circular_book" />


                </RelativeLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/District_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.nic.ODFPlusMonitoring.Support.MyCustomTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/margin_left_20dp"
                    android:layout_marginTop="@dimen/padding"
                    android:layout_marginRight="@dimen/margin_left_20dp"
                    android:text="Select District"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/_15sdp"
                    android:visibility="visible"
                    customfontdemo:fontName="@string/font_circular_medium" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/header"
                    android:layout_marginLeft="@dimen/margin_left_20dp"
                    android:layout_marginTop="@dimen/padding"
                    android:layout_marginRight="@dimen/margin_left_20dp"
                    android:background="@drawable/rectangle_box"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <com.nic.ODFPlusMonitoring.Support.MyCustomTextView

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_centerInParent="true"
                        android:layout_marginLeft="15dp"

                        android:textColor="@android:color/black"
                        android:textSize="@dimen/_15sdp"
                        android:visibility="visible"
                        customfontdemo:fontName="@string/font_circular_book" />

                    <Spinner
                        android:id="@+id/district"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerInParent="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center" />

                </RelativeLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/block_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.nic.ODFPlusMonitoring.Support.MyCustomTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/margin_left_20dp"
                    android:layout_marginTop="@dimen/padding"
                    android:layout_marginRight="@dimen/margin_left_20dp"
                    android:text="Select Block"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/_15sdp"
                    android:visibility="visible"
                    customfontdemo:fontName="@string/font_circular_medium" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/header"
                    android:layout_marginLeft="@dimen/margin_left_20dp"
                    android:layout_marginTop="@dimen/padding"
                    android:layout_marginRight="@dimen/margin_left_20dp"
                    android:background="@drawable/rectangle_box"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <com.nic.ODFPlusMonitoring.Support.MyCustomTextView

